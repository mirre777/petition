
<section>
    <div>
        <form id="profile-form" method="post">
        <h2>Edit your profile</h2>
            <p><strong>{{userprofile.first}} {{userprofile.last}}</strong></p>
            <p><strong>{{userprofile.email}}</strong></p>
            <input id="city" type="text" name="city" placeholder="{{userprofile.city}}" ><br>
            <input id="age" type="number" name="age" placeholder="{{userprofile.age}}" ><br>
            <input id="website" type="text" name="website" placeholder="{{userprofile.website}}"><br>
            <input class="submit-button" type="submit">
        </form>
    </div>
</section>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
<script src="/script-rain.js" charset="utf-8"></script>
